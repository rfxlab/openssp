<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<!-- switch to enable/disable rtb support -->
	<entry key="rtb">true</entry>
	
	<!-- switch to enable/disable ssp support -->
	<entry key="ssp">true</entry>
	
	<!-- switch to enable/disable adserving support -->
	<entry key="adserving">true</entry>
	
	<!-- switch to enable/disable debugging -->	
	<entry key="debug">true</entry>
	
	<!-- switch to enable/disable verbose output -->
	<entry key="verbose">true</entry>
</properties>
